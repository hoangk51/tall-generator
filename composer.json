{
    "name": "hoangk51/livewire-generator",
    "type": "library",
    "description": "Livewire Generator",
    "keywords": [
        "laravel",
        "livewire",
        "api",
        "model",
        "request",
        "migration",
        "model",
        "crud",
        "repository",
        "view",
        "test",
        "generator",
        "swagger"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Pham Huy Hoang",
            "email": "phamhuyhoang1112@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "illuminate/support": "^7.0",
        "laracasts/flash": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "mockery/mockery": "^1.3.1"
    },
    "autoload": {
        "psr-4": {
            "Hoangk51\\Generator\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\Hoangk51\\Generator\\InfyOmGeneratorServiceProvider"
            ]
        }
    }    
}
